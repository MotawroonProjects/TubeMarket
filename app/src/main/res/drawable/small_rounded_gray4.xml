<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="72dp"
        android:height="34dp"
        android:top="34dp">
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" />
            <solid android:color="@color/gray1" />
            <stroke android:width="2dp" android:color="@color/gray2"
                />
        </shape>
    </item>

    <item
        android:width="70dp"
        android:height="33dp"
        android:top="34dp"
        android:left="1dp"
        >
        <shape android:shape="rectangle">
            <corners android:bottomLeftRadius="10dp" android:bottomRightRadius="10dp" />
            <solid android:color="@color/gray1" />

        </shape>
    </item>
    <item
        android:width="60dp"
        android:height="60dp"
        android:left="6dp"
        >
        <shape android:shape="rectangle">
            <corners android:radius="10dp" />
            <solid android:color="@color/color3" />

        </shape>
    </item>


</layer-list>
